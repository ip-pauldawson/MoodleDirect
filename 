[32m+[m[32m$sQuery .= $sWhere;[m[41m[m
[32m+[m[32m$cResult = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) ) ? $DB->get_records_sql( $sQuery, array() ) : get_records_sql( $sQuery  );[m[41m[m
[32m+[m[32m$iTotal = count( $cResult );[m[41m[m
[32m+[m[32m$sQuery .= $sOrder . $sLimit;[m[41m[m
[32m+[m[32m$rResult = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) )[m[41m[m
[32m+[m[32m                ? $DB->get_records_sql( $sQuery, array(), $limitfrom, $limitnum )[m[41m[m
[32m+[m[32m                : get_records_sql( $sQuery, $limitfrom, $limitnum  );[m[41m[m
[32m+[m[32m$iFilteredTotal = $iTotal; //count( $rResult );[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$param_echo = optional_param('sEcho', 0, PARAM_INT);  // echo[m[41m[m
[32m+[m[32m$output = array([m[41m[m
[32m+[m[32m    "sEcho" => $param_echo,[m[41m[m
[32m+[m[32m    "iTotalRecords" => $iTotal,[m[41m[m
[32m+[m[32m    "iTotalDisplayRecords" => $iFilteredTotal,[m[41m[m
[32m+[m[32m    "aaData" => array()[m[41m[m
[32m+[m[32m);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$rResult = ( !is_array( $rResult ) ) ? array() : $rResult;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$i = 0;[m[41m[m
[32m+[m[32mforeach ( $rResult as $result ) {[m[41m[m
[32m+[m[32m    // 'cs.fullname', 'tu.name', 'fl.filename', 'us.firstname', 'us.lastname', 'us.email', 'fl.timecreated'[m[41m[m
[32m+[m[32m    $row = array();[m[41m[m
[32m+[m[32m    $row[] = '<input type="checkbox" id="userlinks_'.$i.'" name="userlinks[]" value="'.$result->id.'" />';[m[41m[m
[32m+[m[32m    $row[] = ( $result->turnitin_uid == 0 ) ? '' : $result->turnitin_uid;[m[41m[m
[32m+[m[32m    $row[] = '&nbsp;';[m[41m[m
[32m+[m[32m    $row[] = ( !is_null( $result->firstname ) ) ? '<a href="'.$CFG->wwwroot.'/user/view.php?id='.$result->userid.'">' .$result->lastname . ', ' . $result->firstname . '</a> (' . $result->email . ')'[m[41m[m
[32m+[m[32m                                                : get_string( 'nonmoodleuser', 'turnitintool' );[m[41m[m
[32m+[m[32m    $row[] = ( $result->turnitin_utp == 1 ) ? turnitintool_pseudoemail( $result->email ) : '-';[m[41m[m
[32m+[m[32m    $row[] = '&nbsp;';[m[41m[m
[32m+[m[32m    $output['aaData'][] = $row;[m[41m[m
[32m+[m[32m    $i++;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
 echo json_encode( $output );[m
