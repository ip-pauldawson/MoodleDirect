[32m+[m[41m[m
[32m+[m[32m$param_sortcol[0] = optional_param('iSortCol_0', null, PARAM_INT);  // sortcol[m[41m[m
[32m+[m[32m$param_sortingcols = optional_param('iSortingCols', 0, PARAM_INT);  // sortingcols[m[41m[m
[32m+[m[32m$sOrder = "";[m[41m[m
[32m+[m[32mif ( !is_null( $param_sortcol[0] ) ) {[m[41m[m
[32m+[m[32m    $sOrder = " ORDER BY ";[m[41m[m
[32m+[m[32m    $startOrder = $sOrder;[m[41m[m
[32m+[m[32m    for ( $i=0; $i < intval( $param_sortingcols ); $i++ ) {[m[41m[m
[32m+[m[32m        $param_sortcol[$i] = optional_param('iSortCol_'.$i, null, PARAM_INT);  // sortcol[m[41m[m
[32m+[m[32m        $param_sortable[$i] = optional_param('bSortable_'.$param_sortcol[$i], null, PARAM_TEXT);  // sortable[m[41m[m
[32m+[m[32m        $param_sortdir[$i] = optional_param('sSortDir_'.$i, null, PARAM_TEXT);  // sortdir[m[41m[m
[32m+[m[32m        if ( $param_sortable[$i] == "true" ) {[m[41m[m
[32m+[m[32m            $sOrder .= $aColumns[ $param_sortcol[$i] ] . " " . $param_sortdir[$i] . ", ";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    $sOrder = substr_replace( $sOrder, "", -2 );[m[41m[m
[32m+[m[32m    if ( $sOrder == $startOrder ) $sOrder = "";[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[32m$sOrder .= ",tu.id asc ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$param_search = optional_param('sSearch', null, PARAM_TEXT);  // sortingcols[m[41m[m
[32m+[m[32m$start = true;[m[41m[m
[32m+[m[32m$sWhere = ' AND ( ';[m[41m[m
[32m+[m[32m$nobracket = false;[m[41m[m
[32m+[m[32mfor ( $i=0; $i < count($aColumns); $i++ ) {[m[41m[m
[32m+[m[32m    $param_searchable[$i] = optional_param('bSearchable_'.$i, null, PARAM_TEXT);[m[41m[m
[32m+[m[32m    $param_search_n[$i] = optional_param('sSearch_'.$i, null, PARAM_TEXT);[m[41m[m
[32m+[m[32m    if ( !is_null($param_searchable[$i]) && $param_searchable[$i] == "true" && ( $param_search != '' OR $param_search_n[$i] != '' ) ) {[m[41m[m
[32m+[m[32m        if ( !$start ) $sWhere .= " OR ";[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        if ( $aColumns[$i] == 'sb.submission_objectid' AND $param_search_n[$i] == '##deletable##' ) {[m[41m[m
[32m+[m[32m            $sWhere = ( $sWhere == ' AND ( ' ) ? '' : substr_replace( $sWhere, "", -3 ) . ' )';[m[41m[m
[32m+[m[32m            $sWhere .= " AND ( sb.submission_objectid IS NOT NULL OR sb.submission_filename IS NULL )";[m[41m[m
[32m+[m[32m            $nobracket = true;[m[41m[m
[32m+[m[32m        } else if ( $aColumns[$i] != ' ' ) {[m[41m[m
[32m+[m[32m            $sWhere .= "CAST(" . $aColumns[$i] . " AS CHAR) LIKE '%" . $param_search . "%'";[m[41m[m
[32m+[m[32m            $start = false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[32mif ( $sWhere != ' AND ( ' AND !$nobracket ) {[m[41m[m
[32m+[m[32m    $sWhere .= " ) ";[m[41m[m
[32m+[m[32m} else if ( $nobracket ) {[m[41m[m
[32m+[m[32m    $sWhere .= " ";[m[41m[m
[32m+[m[32m} else {[m[41m[m
[32m+[m[32m    $sWhere = "";[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$sLimit = "";[m[41m[m
[32m+[m[32mif ( !is_null( $param_displaystart ) && $param_displaylength != '-1' ) {[m[41m[m
[32m+[m[32m    $limitfrom = $param_displaystart;[m[41m[m
[32m+[m[32m    $limitnum  = $param_displaylength;[m[41m[m
[32m+[m[32m} else {[m[41m[m
[32m+[m[32m    $limitfrom = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) ) ? 0 : '';[m[41m[m
[32m+[m[32m    $limitnum  = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) ) ? 0 : '';[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$sQuery .= $sWhere;[m[41m[m
[32m+[m[32m$cResult = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) ) ? $DB->get_records_sql( $sQuery, array() ) : get_records_sql( $sQuery );[m[41m[m
[32m+[m[32m$iTotal = count( $cResult );[m[41m[m
[32m+[m[32m$sQuery .= $sOrder . $sLimit;[m[41m[m
[32m+[m[32m$rResult = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) )[m[41m[m
[32m+[m[32m                ? $DB->get_records_sql( $sQuery, array(), $limitfrom, $limitnum )[m[41m[m
[32m+[m[32m                : get_records_sql( $sQuery, $limitfrom, $limitnum  );[m[41m[m
[32m+[m[32m$iFilteredTotal = $iTotal; //count( $rResult );[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$param_echo = optional_param('sEcho', 0, PARAM_INT);  // echo[m[41m[m
[32m+[m[32m$output = array([m[41m[m
[32m+[m[32m    "sEcho" => $param_echo,[m[41m[m
[32m+[m[32m    "iTotalRecords" => $iTotal,[m[41m[m
[32m+[m[32m    "iTotalDisplayRecords" => $iFilteredTotal,[m[41m[m
[32m+[m[32m    "aaData" => array()[m[41m[m
[32m+[m[32m);[m[41m[m
[32m+[m[41m[m
[32m+[m[32mforeach ( $rResult as $result ) {[m[41m[m
[32m+[m[32m    // 'cs.fullname', 'tu.name', 'fl.filename', 'us.firstname', 'us.lastname', 'us.email', 'fl.timecreated'[m[41m[m
[32m+[m[32m    if ( !empty( $param_search ) AND !is_null( $result->unanon ) AND !$result->unanon ) {[m[41m[m
[32m+[m[32m        $output['iTotalDisplayRecords'] = $output['iTotalDisplayRecords'] - 1;[m[41m[m
[32m+[m[32m        continue; // If these are search results and this is anonymised skip it[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    $row = array();[m[41m[m
[32m+[m[32m    if ( is_null( $result->filename ) OR is_null( $result->activityid ) ) {[m[41m[m
