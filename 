[32m+[m[32m    '.$CFG->prefix.'turnitintool tu ON cm.instance = tu.id[m[41m[m
[32m+[m[32mLEFT JOIN[m[41m[m
[32m+[m[32m    '.$CFG->prefix.'course cs ON tu.course = cs.id[m[41m[m
[32m+[m[32mWHERE[m[41m[m
[32m+[m[32m    fl.component = \'mod_turnitintool\' AND fl.filesize != 0';[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$sCountQuery = 'SELECT[m[41m[m
[32m+[m[32m    fl.id AS id[m[41m[m
[32m+[m[32mFROM[m[41m[m
[32m+[m[32m    '.$CFG->prefix.'files fl[m[41m[m
[32m+[m[32mLEFT JOIN[m[41m[m
